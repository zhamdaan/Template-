#include <iostream>
using namespace std  ;  
 template <class T>
 void max3  (T s,T k , T y)  
     {   T a,b,c  ;  
        a=s;
        k=b;
        c=y;
        if (a>b&&a>c)
         { cout<<"the greatest number is "<<a; 
         }  
          else if (b>a&&b>c)
          { cout<<"the greatest number is   "<<b;  
          }  
        else if(c>>a&&c>b)
         {  cout<<"the greatest number is "<<c ;  
         }  
         cout<<endl ; 
     } 
     void max3 (int k)
      {  cout<<endl<<"explicit function , overloading "<<k  ; }
int main() {
   max3 (60,5,2) ;
   max3 ('A','b','Z' );
   max3 (1);
    return 0;
}
